{
  "presets": [
    "@babel/react",
    ["@babel/env", { "modules": false, "useBuiltIns": "usage" }]
  ],
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "displayName": true
      }
    ],
    "@babel/plugin-transform-regenerator",
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread"
  ]
}
